<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link rel="stylesheet" href="~/layout/bootstrap.css" />
    <link rel="stylesheet" href="~/layout/bootstrap.min.css" />
    <link rel="stylesheet" href="~/layout/AdminLTE.min.css" />
    <link rel="stylesheet" href="~/layout/_all-skins.min.css" />
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.13/css/jquery.dataTables.min.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.13/js/dataTables.bootstrap.min.js">
   
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <link rel="stylesheet" href="~/layout/bootstrap3-wysihtml5.min.css" />
    <link rel="stylesheet" href="https://use.fontawesome.com/526b30efc7.css">
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")

   
    <script src="https://cdn.datatables.net/1.10.13/js/jquery.dataTables.min.js" type="text/javascript"></script>
    @RenderSection("scripts", required: false)

</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>Admin</b></span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">

                <!-- Sidebar toggle button-->
              <!--  <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a> -->

                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- Messages: style can be found in dropdown.less-->
                        <li style="padding-right:39px;">
                           
                            @Html.Partial("_LoginPartial")
                            
                            
                        </li>
                    </ul>
                </div>

            </nav>
        </header>

        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar" style="height: auto;">
                <ul class="sidebar-menu">
                    <li class="header">MAIN NAVIGATION</li>

                    <li>
                        <a href='@Url.Action("Index", "client_info")'>
                            <i class="fa fa-user-o"></i> <span>Clients</span>

                        </a>
                    </li>


                    <li>
                        <a href='@Url.Action("Index", "enquiry_info")'>
                            <i class="fa fa-pencil-square-o"></i> <span>Inquiries</span>

                        </a>
                    </li>
                    <li>
                        <a href='@Url.Action("Index", "proposal_info")'>
                            <i class="fa fa-handshake-o "></i> <span>Proposal</span>

                        </a>
                    </li>
                    <li>
                        <a href='@Url.Action("Index", "invoice_main")'>
                            <i class="fa fa-print "></i> <span>Invoice</span>

                        </a>
                    </li>

                </ul>

            </section>
            <!-- /.sidebar -->
        </aside>

        <div class="content-wrapper">
            @RenderBody()
            

        </div>
        <footer class="main-footer">
            <div class="pull-right hidden-xs">

            </div>
            All rights
            reserved.
        </footer>


        @Scripts.Render("~/bundles/bootstrap")
    </div>
</body>
</html>
